      "source": [
        "# Assuming '168' is the userId for 'user_2' based on the previous code\n",
        "user_1 = df[(df['userId']==3)]  # Use 'df' instead of 'movies_df'\n",
        "user_2 = df[(df['userId']==168)]  # Use 'df' instead of 'movies_df'\n",
        "# Now you can access the movie column for user_1\n",
        "# Now you can access the movie column for user_2\n",
        "user_1.movie\n",
        "user_2.movie"
      ],
